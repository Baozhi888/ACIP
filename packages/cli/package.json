  "devDependencies": {
    "@types/fs-extra": "^11.0.1",
    "@types/inquirer": "^9.0.3",
    "@types/jest": "^29.5.3",
    "@types/node": "^20.4.2",
    "@types/semver": "^7.5.0",
    "@types/update-notifier": "^6.0.4",
    "@typescript-eslint/eslint-plugin": "^6.1.0",
    "@typescript-eslint/parser": "^6.1.0",
    "eslint": "^8.45.0",
    "jest": "^29.6.1",
    "prettier": "^3.0.0",
